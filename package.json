{
  "name": "repo",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "web": "pnpm --filter @repo/web",
    "ui": "pnpm --filter @repo/ui ui",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "husky": "^9.1.7",
    "prettier": "catalog:",
    "turbo": "2.1.1"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20.17.0"
  },
  "volta": {
    "node": "20.17.0",
    "pnpm": "9.12.0"
  },
  "corepack": {
    "enabled": true
  }
}
